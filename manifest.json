{
  "manifest_version": 3,
  "name": "MAL Manga Info",
  "version": "1.0.1",
  "description": "An extension to retrieve the basic informations of a manga on MyAnimeList",
  "homepage_url": "https://github.com/RettaInGit/MAL-manga-info",
  "content_scripts": [ {
        "matches": ["https://myanimelist.net/manga/*"],
        "js": ["main_js.js"]
      } ],
  "action": {
      "default_title": "MAL Manga Info",
      "default_popup": "popup_html.html",
      "default_icon": {
        "16": "images/16px.png",
        "24": "images/24px.png",
        "32": "images/32px.png"
      }
    },
  "icons": {
      "16": "images/16px.png",
      "48": "images/48px.png",
      "128": "images/128px.png"
    }
}
